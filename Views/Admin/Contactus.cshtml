@model FishingTournament02.Models.ContactUs

<h2>Contact Us</h2>
<form asp-action="" asp-controllers="About US" method="post">

	<div>
		<label asp-for="Name">Name:</label>
		<input type="text" asp-for="Name" />
	</div>
	<div>
		<label asp-for="Email_Address">Email Address:</label>
		<input type="text" asp-for="Email_Address" />
	</div>
	<div>
		<label asp-for="Message">Enter your message:</label>
		<textarea asp-for="Message"></textarea>
	</div>

	<input type="submit" value="Send" />
</form>