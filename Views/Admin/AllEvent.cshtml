<p><a asp-controller="Admin" asp-action="AddEvent">Add an event</a></p>



@{
    if (Model.Count() < 1)
    {
        <p>No events in database yet.</p>
    }
    else
    {
        <h3>All Events</h3>
        <div class="table-responsive">
            <table class="table table-striped table-hover">

                <tr>
                    <th>Event Name</th>
                    <th>Event Address</th>
                    <th>Event City</th>
                    <th>Event State</th>
                    <th>Event ZipCode</th>
                    <th>Event Capacity</th>
                    <th>Event URL</th>
                    <th>Event Cost</th>

                </tr>
                @foreach (var events in Model)
                {
                    <tr>
                        <td>@events.EventName</td>
                    </tr>
                }
            </table>
        </div>

    }
}

